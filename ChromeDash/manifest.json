{
    "manifest_version": 2,
    "name": "SmartRoom ChromeDash",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "SmartRoom ChromeDash",
        "default_popup": "dashboard.html"
    }
}